{
  "name": "angularjs-ts-seed",
  "version": "1.12.0",
  "description": "Angularjs 1.5 TypeScript Seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/fr-esco/angularjs-ts-seed"
  },
  "main": "electron.conf.js",
  "scripts": {
    "clean": "ts-node ./scripts/clean.ts",
    "lint": "npm-run-all -p lint:*",
    "lint:html": "htmllint 'app/**/*.html'",
    "lint:style": "sass-lint -c .sass-lint.yml -v",
    "lint:ts": "tslint --project tsconfig.json",
    "preserve.dev": "gulp build",
    "serve.dev": "webpack-dev-server --config webpack.config.dev --progress --profile --watch-poll --inline --hot",
    "preserve.prod": "gulp build --prod",
    "serve.prod": "webpack-dev-server -p --config webpack.config.prod --progress --profile --watch-poll --inline --hot",
    "build": "ts-node ./scripts/build.ts",
    "serve": "npm run serve.dev",
    "prebuild.cordova": "npm run build.dev",
    "build.cordova": "gulp cordova --task build --env dev",
    "git.merge": "git fetch upstream && git merge upstream/master",
    "git.merge.preview": "git fetch upstream && git merge --no-commit upstream/master",
    "git.prepare": "git add . && git commit -m'setup'",
    "git.setup": "git init && git remote add upstream https://github.com/fr-esco/angularjs-ts-seed.git",
    "gulp.update": "npm i -D --progress=false gulp gulp-autoprefixer gulp-bump gulp-clean-css gulp-cli gulp-concat gulp-electron-packager gulp-filter gulp-folders gulp-htmllint gulp-if gulp-ignore gulp-inject gulp-livereload gulp-load-plugins gulp-merge-json gulp-minify-html gulp-ng-annotate gulp-ng-constant gulp-ng-html2js gulp-notify gulp-plumber gulp-preprocess gulp-rename gulp-replace gulp-sass gulp-sourcemaps gulp-strip-comments gulp-template gulp-tslint gulp-typescript gulp-uglify gulp-util gulp-watch gulp-zip",
    "ng.update": "npm i -S angular angular-animate angular-aria angular-cookies angular-i18n angular-message-format angular-messages angular-mocks angular-sanitize angular-touch",
    "typings.update": "typings prune && typings install angular angular-animate angular-component-router angular-cookies angular-dynamic-locale angular-hotkeys angular-material angular-mocks angular-resource angular-sanitize angular-translate es6-shim jasmine jasmine-expect jquery node nvd3 restangular shelljs yargs --save --global --source=dt",
    "postinstall": "typings install && gulp -T",
    "test": "echo \"Error: no test specified\" && exit 1",
    "hello": "node ./scripts/hello.js"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "es6",
    "systemjs",
    "seed",
    "application"
  ],
  "author": "fr-esco <f.colamonici@gmail.com>",
  "contributors": [
    {
      "name": "bsorrentino",
      "email": "bartolomeo.sorrentino@gmail.com"
    },
    {
      "name": "fr-esco",
      "email": "f.colamonici@gmail.com"
    }
  ],
  "license": "MIT",
